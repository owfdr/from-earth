{
  "name": "from-earth",
  "productName": "from-earth",
  "version": "1.1.0",
  "description": "Explore beautiful places on Earth",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform=darwin --arch=universal",
    "make:deb": "electron-forge make --platform=linux --arch=x64 --targets=deb",
    "make:linux": "electron-forge make --platform=linux --arch=x64,arm64",
    "make:windows": "electron-forge make --platform=win32 --arch=x64,arm64",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "release": "standard-version"
  },
  "keywords": [],
  "author": {
    "name": "owfdr",
    "email": "owfdr@ourworld.center"
  },
  "license": "MIT",
